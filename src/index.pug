<!DOCTYPE html>
html(lang="en")
   head
      meta(charset="UTF-8")
      meta(name="viewport", content="width=device-width, initial-scale=1.0")
      link(rel="stylesheet", href="css/normalize.css")
      link(rel="stylesheet", href="css/style.css")
      title Calculator
   body
      section(class="wrapper")
         div(class="calc")
            div(class="calc__screen")
               input(type="text" placeholder="0")
            div(class="calc__keybord")
               span(class="calc__number" id="n_0")
                  span 0
               p(class="calc__number" id="n_1")
                  span 1
               p(class="calc__number" id="n_2")
                  span 2
               p(class="calc__number" id="n_3")
                  span 3
               p(class="calc__number" id="n_4")
                  span 4
               p(class="calc__number" id="n_5")
                  span 5
               p(class="calc__number" id="n_6")
                  span 6
               p(class="calc__number" id="n_7")
                  span 7
               p(class="calc__number" id="n_8")
                  span 8
               p(class="calc__number" id="n_9")
                  span 9
               p(class="calc__number" id="ac")
                  span AC
               p(class="calc__number" id="larr")
                  span &larr;
               p(class="calc__number" id="radic")
                  span &radic;
               p(class="calc__number" id="sup2")
                  span x&sup2;
               p(class="calc__number" id="plus")
                  span +
               p(class="calc__number" id="minus")
                  span -
               p(class="calc__number" id="times")
                  span &times;
               p(class="calc__number" id="divide")
                  span &divide;
               p(class="calc__number" id="equal")
                  span =
               p(class="calc__number" id="point")
                  span .
      //- script(src="js/getNumbers.js")